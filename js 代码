11.
//判断一个字符串是否以 born 开头
var paragraph = "born 15-11-2003 (mother spot):white fang";
paragraph.charAt(0) == "b" && paragraph.charAt(1) == "o" &&paragraph.charAt(2) == "r" && paragraph.charAt(3) == "n";
//true

paragraph.slice(0,4) == "born";
//true

paragraph.slice(0,3) == "born";
//false

12.
function startWith(string,pattern){
  return string.slice(0,pattern.length)  == pattern;
}
undefined
startWith("rotation","rot");
//true
startWith("rot","rotation");
//false
13.
//提取文字
function catNames(paragraph){
var colon = paragraph.indexOf(":");
return colon;
}
//undefined
catNames("born 20/09/2004 (mother Yellow Bess):doctor hobbles the 2nd, noog");
//36

function catNames(paragraph){
var colon = paragraph.indexOf(":");
return paragraph.slice(colon+2).split(", ");
}
//undefined
catNames("born 20/09/2004 (mother Yellow Bess):doctor hobbles the 2nd, noog");
//(2) ["octor hobbles the 2nd", "noog"]

14.
var url="http://baidu.com/index.txt";
	function getFileFormat(url){
		//获取.在url中出现的位置
		var pos=url.lastIndexOf(".");
		return url.substr(pos);
	}
	var  formatName=getFileFormat(url);
	console.log(formatName);
	var picFormat=getFileFormat("1231435.jpg");
	console.log(picFormat);
// .txt
// .jpg

undefined
var url="http://baidu.com/index.txt";
	function getFileFormat(url){
		//获取.在url中出现的位置
		var pos=url.lastIndexOf(".");
		return url.substr(pos+1);
	}
	var  formatName=getFileFormat(url);
	console.log(formatName);
	var picFormat=getFileFormat("1231435.jpg");
	console.log(picFormat);
// txt
// jpg

